local Player = game:GetService("Players").LocalPlayer

-- Function to format numbers with commas
local function formatCurrency(value)
    local formatted = tostring(value)
    while true do
        formatted, k = formatted:gsub("^(-?%d+)(%d%d%d)", '%1,%2')
        if k == 0 then
            break
        end
    end
    return formatted
end

local function updateCurrency()
    local currencyValue = Player:WaitForChild("DataFolder"):WaitForChild("Currency").Value
    textLabel.Text = "Currency: " .. formatCurrency(currencyValue)
end

-- Update the currency display initially and whenever it changes
updateCurrency()
Player.DataFolder.Currency:GetPropertyChangedSignal("Value"):Connect(updateCurrency)

repeat task.wait(0.1) until game:IsLoaded() and game:GetService("Players").LocalPlayer

